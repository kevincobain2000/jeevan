<%= include_gon %>
<div id="main" role="main">
  <%= render :partial =>
  "layouts/ribbon", :locals => {:papa => "Home", :son => "Dashboard"} %>
  <!-- MAIN CONTENT -->
  <div id="content">
    <div class="row">
      <div class="col-xs-12 col-sm-7 col-md-7 col-lg-4">
        <h1 class="page-title txt-color-blueDark">
          <i class="fa fa-desktop fa-fw "></i>
          UI Elements
          <span>
            JQuery UI
          </span>
        </h1>
      </div>
      <div class="col-xs-12 col-sm-5 col-md-5 col-lg-8">
        <%= render :partial =>'layouts/sparks', :locals => {:user => @user}%>
      </div>
    </div>
    <!-- row -->
    <div class="row">
      <div class="col-sm-12">

        <div class="well">
          <h1>People Visited my Profile</h1>
          <table class="table table-striped table-bordered" id="inv">
            <thead>
              <tr>
                <th>Image</th>
                <th>Profile</th>
                <th>Religion</th>
                <th>Location</th>
<!--                 <th>Age</th> -->
                <th>Posted By</th>
                <th>Last Online</th>
              </tr>
            </thead>
            <tbody>
            </tbody>
          </table>
        </div>
      </div>
      <!-- end row -->
    </div>
  </div>
</div>
