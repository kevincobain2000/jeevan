<%= include_gon %>
<div id="main" role="main">
  <%= render :partial =>
  "layouts/ribbon", :locals => {:papa => "Home", :son => "Dashboard"} %>
  <!-- MAIN CONTENT -->
  <div id="content">
    <div class="row">
      <div class="col-xs-12 col-sm-7 col-md-7 col-lg-4">
        <h1 class="page-title txt-color-blueDark">
          <i class="fa fa-desktop fa-fw "></i>
          UI Elements
          <span>
            JQuery UI
          </span>
        </h1>
      </div>
      <div class="col-xs-12 col-sm-5 col-md-5 col-lg-8">
      <%= link_to edit_profile_path(:id =>current_user.profile.id), :class=>"btn btn-default pull-right" do %>
        <i class="fa fa-eye"></i> Edit My Profile
        <% end %>
        <%= render :partial =>'layouts/sparks', :locals => {:user => @user}%>
      </div>
    </div>
    <!-- row -->
    <div class="row">
      <div class="col-sm-12">

        <div class="well">
          <h3 class="text-primary">People Visited my Profile <small>Secondary text</small></h3>
          <table class="table table-striped table-bordered table-hover" id="in_interests">
            <thead>
              <tr>
                <th><i class="fa fa-picture-o"></i> Image</th>
                <th><i class="fa fa-picture-o"></i> Profile</th>
                <th><i class="fa fa-picture-o"></i> Religion</th>
                <th><i class="fa fa-picture-o"></i> Location</th>
                <th><i class="fa fa-picture-o"></i> Age</th>
                <th><i class="fa fa-picture-o"></i> Posted By</th>
                <th><i class="fa fa-picture-o"></i> Last Online</th>
              </tr>
            </thead>
            <tbody>
            </tbody>
          </table>
        </div>
        <!-- End well -->
        <div class="well">
          <h3 class="text-primary">People Visited my Profile <small>Secondary text</small></h3>
          <table class="table table-striped table-bordered" id="out_interests">
            <thead>
              <tr>
                <th><i class="fa fa-picture-o"></i> Image</th>
                <th><i class="fa fa-picture-o"></i> Profile</th>
                <th><i class="fa fa-picture-o"></i> Religion</th>
                <th><i class="fa fa-picture-o"></i> Location</th>
                <th><i class="fa fa-picture-o"></i> Age</th>
                <th><i class="fa fa-picture-o"></i> Posted By</th>
                <th><i class="fa fa-picture-o"></i> Last Online</th>
              </tr>
            </thead>
            <tbody>
            </tbody>
          </table>
        </div>
        <!-- End well -->
        <div class="well">
          <h3 class="text-primary">People Visited my Profile <small>Secondary text</small></h3>
          <table class="table table-striped table-bordered" id="in_visitors">
            <thead>
              <tr>
                <th><i class="fa fa-picture-o"></i> Image</th>
                <th><i class="fa fa-picture-o"></i> Profile</th>
                <th><i class="fa fa-picture-o"></i> Religion</th>
                <th><i class="fa fa-picture-o"></i> Location</th>
                <th><i class="fa fa-picture-o"></i> Age</th>
                <th><i class="fa fa-picture-o"></i> Posted By</th>
                <th><i class="fa fa-picture-o"></i> Last Online</th>
              </tr>
            </thead>
            <tbody>
            </tbody>
          </table>
        </div>
        <!-- End well -->
      </div>
      <!-- end row -->
    </div>
  </div>
</div>
