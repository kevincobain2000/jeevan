<% categories = ['birth_country', 'birth_city', 'dob', 'tob', 'manglik', 'sub_sign', 'moon_sign', 'nakshatra'] %>
<dl class="dl-horizontal">
  <% categories.each do |category| %>
    <% heading = category.gsub "_", " " %>
    <dt><%= heading.capitalize %>:</dt>
    <dd><%= @user[:kundali][category] %></dd>
  <% end %>
</dl>
