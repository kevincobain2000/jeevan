<%#= render :partial =>"layouts/sparks2" %>
<%#= render :partial =>"layouts/search"%>
  <!-- MAIN CONTENT -->
  <div id="content"class="font-sm">
    <h1>
      <span class="semi-bold"><%= h %></span>
      <%= hsm %>
      <small>total found in this category (<%= paginate.total_entries %>)</small>
    </h1>

      <%= will_paginate paginate, :class =>"flickr_pagination"%>
      <legend></legend>
      <% if paginate.empty? %>
      <div class="error-box">
        <h3 class="text-center"><i class="fa fa-fw fa-bolt fa-lg"></i> Sorry <br><small>Currently no profiles in this section <a href="/">go and explore more profiles</a></small></h3>
      </div>
      <% end %>
      <% paginate.each do |_user| %>
        <% user = make_user_card(_user) %>
        <%= render :partial =>'layouts/card.html.erb', :locals => {:user => user} %>
          <% end %>
          <legend></legend>
          <%= will_paginate paginate, :class =>"flickr_pagination"%>
    </div>