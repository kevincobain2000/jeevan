<legend></legend>
<% if  current_user.id != user[:id] && !user[:out_response].nil? %>
  <% if user[:out_response].response == 1 %>
    <dl class="dl-horizontal pull-left font-sm">
      <dt>
        <i class='fa fa-phone'></i>
        Telephone:
      </dt>
      <dd>
        <%= user[:contact][:self_no]  %>
      </dd>

      <dt>
        <i class='fa fa-skype txt-color-blue'></i>
        Skype:
      </dt>
      <dd>
        <%= user[:contact][:s_id]  %>
      </dd>

      <dt>
        <i class='fa fa-google-plus txt-color-red'></i>
        Google:
      </dt>
      <dd>
        <%= user[:contact][:g_id]  %>
      </dd>

      <dt>
        <i class='fa fa-comment txt-color-green'></i>
        WhatsApp:
      </dt>
      <dd>
        <%= user[:contact][:w_id]  %>
      </dd>

    </dl>
  <% else %>
  <dl class="dl-horizontal pull-left font-sm">
    <dt>
    <i class='fa fa-phone'></i>
      Telephone:
    </dt>
