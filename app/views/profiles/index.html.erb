<div id="main" role="main">

<%#= render :partial =>"layouts/sly" %>
<%= render :partial =>"layouts/ribbon", :locals => {:papa => "Home", :son => "Explore"} %>
<%#= render :partial =>"layouts/sparks", :locals => {:sparks => @sparks} %>


<% if !current_user.avatar_updated_at %>
	<br><br>
	<%= render :partial =>'layouts/profiles/edit/avatar'%>
	<% elsif current_user.images_count.to_i < 1 %>
	<br><br>
	<%= render :partial =>'layouts/profiles/edit/images'%>
	<br><br>
	<% elsif current_user.profile[:home] == nil %>
	<br><br>
	<%= render :partial =>'layouts/profiles/edit/basic'%>
<% end %>


<%= render :partial =>"layouts/profiles/pin", :locals => {:paginate => @matching, :h => "Matching", :hsm => "Profiles"} %>
