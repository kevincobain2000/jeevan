<div id="main" role="main">
  <%= render :partial =>"layouts/ribbon", :locals => {:papa => "Home", :son => "Explore"} %>
  <!-- MAIN CONTENT -->
  <div id="content">
    <div id="wrapper">
      <h1>
        <span class="semi-bold">Matching</span>
        Profiles</h1>
      <div id="columns">
        <% @profiles_paginate_matches.each do |user| %>
          <%= render :partial =>'layouts/searchsnippets', :locals=>{:user => make_user(user)} %>
        <% end %>
      </div>
      <%= will_paginate @profiles_paginate_matches, :class =>"flickr_pagination", :param_name => "matches" %>
    </div>
  </div>
</div>
